# programs
alt + Return
    st -T 'Simple Terminal' -t 'Simple Terminal' -c 'Simple Terminal'
alt + Shift + Return
    stwrapper
alt + w
    firefox
alt + shift + w
    chromium
control + alt + shift + w
    start-tor-browser
alt + r
    st -c ranger -e ranger --cmd='set viewmode multipane'
alt + x
    slock
Print
    scrap -n
control + Print
    scrap -u
alt + e
    pcmanfm
alt + v
    virt-manager

# dmenus
alt + p
    menu run -p 'Run:'
alt + o
    dedit
alt + i
    brws
alt + shift + i
    brws -a
alt + u
    dmoji
alt + c
    clip

# misc
control + alt + k
    keys
shift + Delete
    xsel -o

# brightness
XF86MonBrightnessUp
    bright i
XF86MonBrightnessDown
    bright d

# redshift control
alt + equal
    sctc i
alt + minus
    sctc d

# volume
alt + apostrophe
    vol i
alt + semicolon
    vol d
XF86VolumeUp
    vol i
XF86VolumeDown
    vol d

# media / mpv
alt + slash
    mmt -t
alt + period
    mmt -n
alt + comma
    mmt -p
alt + bracketright
    mmt -f
alt + bracketleft
    mmt -b
