# programs
# alt + Return
    # st -T 'Simple Terminal' -t 'Simple Terminal' -c 'Simple Terminal'
    #st -T 'Simple Terminal' -t 'Simple Terminal' -c 'Simple Terminal'
alt Return
    stwrapper
alt + w
    google-chrome
alt + shift + w
    firefox
control + alt + shift + w
    start-tor-browser
alt + r
    st -c ranger -e ranger --cmd='set viewmode multipane'
control + alt + x
    slock
Print
    scrap -n
control + Print
    scrap
alt + e
    pcmanfm
alt + v
    virt-manager

# dmenus
alt + p
    menu run -p 'Run:'
alt + o
    dedit
# alt + i
#     brws
# alt + shift + i
#     brws -a
alt + u
    dapp
alt + y
    dmoji
alt + c
    clip

# misc
control + alt + k
    keys
shift + Delete
    xsel -o

# brightness
XF86MonBrightnessUp
    bright i
XF86MonBrightnessDown
    bright d

# redshift control
alt + equal
    sctc i
alt + minus
    sctc d

# volume
alt + apostrophe
    vol i
alt + semicolon
    vol d
XF86VolumeUp
    vol i
XF86VolumeDown
    vol d

# media / mpv
alt + slash
    mmt -t
alt + period
    mmt -n
alt + comma
    mmt -p
alt + bracketright
    mmt -f
alt + bracketleft
    mmt -b

# --------- rs shit ---------------
control + shift + F12
    rs_autotyper.sh

control + Escape
    autotype_given.sh '::renderself'

control + Insert
    autotype_given.sh 'flash3:#rejectz on my eyelidz famalam'
