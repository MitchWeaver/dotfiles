# programs
alt + Return
    st
alt + shift + Return
    tabbed -d -c -r 2 st -w ''
alt + w
    browser
alt + r
    st -T floating-st -n floating-st -e ranger
alt + x
    slock
Print
    scrap -n
control + Print
    scrap
alt + e
    pcmanfm

# dmenus
alt + p
    menu run -p 'Run:'
alt + o
    dedit
alt + u
    tasks
alt + c
    clip

# misc
control + alt + k
    keys
shift + Delete
    xsel -o
alt + Delete
    touchpad

# volume / media
alt + apostrophe
    vol -inc 8
alt + semicolon
    vol -dec 8
alt + slash
    mpvc toggle
alt + period
    mpvc --track +1
alt + comma
    mpvc --track -1
alt + bracketright
    mpvc --seek +30
alt + bracketleft
    mpvc --seek -30
