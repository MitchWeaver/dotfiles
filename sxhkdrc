# programs
alt + Return
    tabbed -d -c -r 2 st -w ''
alt + w
    tabbed -d -c surf -e
alt + shift + w
    chromium --new-window
alt + r
    st -T floating-st -n floating-st -e ranger
alt + x
    slock -m "$(bonsai2)"
Print
    scrap -n
control + Print
    scrap

# dmenus
alt + p
    menu run -p 'Run:'
alt + o
    dedit
alt + u
    tasks
alt + c
    clip
alt + i
    mus -m

# misc
alt + shift + k
    keys

# volume / media
alt + apostrophe
    vol -inc 2
alt + semicolon
    vol -dec 2
alt + slash
    media -t
alt + period
    media -n
alt + comma
    media -p
alt + bracketright
    media -sa
alt + bracketleft
    media -sb
